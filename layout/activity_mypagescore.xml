                android:fontFamily="@font/binggrae"
                android:text="jammin"
                android:textColor="#656363"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/myscore_point1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/binggrae"
                android:text="5150"
                android:textColor="#656363"
                android:textSize="25sp" />
        </TableRow>

        <TableRow
