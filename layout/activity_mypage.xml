<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mypage">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cloud" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="409dp"
        android:layout_height="180dp"
        android:layout_marginTop="476dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dog" />
    <Button
        android:id="@+id/mypage_back"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#FFFFFF"
        android:foreground="@drawable/arrow"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:background="#00FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/mypage_out"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#00FFFF"
        android:foreground="@drawable/house"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="150dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="150dp"
        android:fontFamily="@font/bunggrae_bold"
        android:text="My Page"
        android:textColor="#656363"
        android:textSize="35sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/mypage_btn_plant"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="132dp"
        android:backgroundTint="#57BA8C"
        android:foreground="@drawable/bee"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:cornerRadius="75dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/bunggrae_bold"
        android:text="출석!!"
        android:textColor="#656363"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/mypage_btn_plant"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="@+id/mypage_btn_plant"
        app:layout_constraintTop_toBottomOf="@+id/mypage_btn_plant" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bunggrae_bold"
        android:text="얼마나 자랐니?"
        android:textColor="#656363"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView34"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="@+id/textView34"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <Button
        android:id="@+id/mypage_btn_album"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="76dp"
        android:backgroundTint="#5EC0E6"
        android:foreground="@drawable/pingu"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:cornerRadius="75dp"
        app:layout_constraintBottom_toBottomOf="@+id/mypage_btn_plant"
        app:layout_constraintStart_toEndOf="@+id/mypage_btn_plant"
        app:layout_constraintTop_toTopOf="@+id/mypage_btn_plant"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/bunggrae_bold"
        android:text="앨범"
        android:textColor="#656363"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/mypage_btn_album"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="@+id/mypage_btn_album"
        app:layout_constraintTop_toBottomOf="@+id/mypage_btn_album" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bunggrae_bold"
        android:text="이렇게 찍었어요!"
        android:textColor="#656363"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView36"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="@+id/textView36"
        app:layout_constraintTop_toBottomOf="@+id/textView36" />

    <Button
        android:id="@+id/mypage_btn_score"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="90dp"
        android:backgroundTint="#9061C6"
        android:foreground="@drawable/panda"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:cornerRadius="75dp"
        app:layout_constraintEnd_toEndOf="@+id/mypage_btn_plant"
        app:layout_constraintStart_toStartOf="@+id/mypage_btn_plant"
        app:layout_constraintTop_toBottomOf="@+id/mypage_btn_plant" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/bunggrae_bold"
        android:text="순위표"
        android:textColor="#656363"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/mypage_btn_score"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="@+id/mypage_btn_score"
        app:layout_constraintTop_toBottomOf="@+id/mypage_btn_score" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bunggrae_bold"
        android:text="우와! 나 잘했죠?"
        android:textColor="#656363"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/mypage_btn_logout"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="76dp"
        android:backgroundTint="#F8DB4B"
        android:foreground="@drawable/owl"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:cornerRadius="75dp"
        app:layout_constraintBottom_toBottomOf="@+id/mypage_btn_score"
        app:layout_constraintStart_toEndOf="@+id/mypage_btn_score"
        app:layout_constraintTop_toTopOf="@+id/mypage_btn_score"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/bunggrae_bold"
        android:text="로그아웃"
        android:textColor="#656363"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/mypage_btn_logout"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="@+id/mypage_btn_logout"
        app:layout_constraintTop_toBottomOf="@+id/mypage_btn_logout" />

</androidx.constraintlayout.widget.ConstraintLayout>